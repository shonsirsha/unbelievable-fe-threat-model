									"status": "Open",
									"severity": "High",
									"title": "CSRF Attacks",
									"type": "Spoofing",
									"description": "Any client could forge requests to our API (backend).",
									"mitigation": "Backend should enforce CORS headers to only allow frontend's domain.",
									"modelType": "STRIDE"
								}
							],
							"hasOpenThreats": true,
